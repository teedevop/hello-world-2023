pipeline {
    agent any
     stages{
      stage('Hello'){
       steps {
         echo "Hello World"
        }
      }  
      stage('Build') {
          steps {
              echo 'Build step'
              sleep 10
          }    
       }
       stage('Test') {
           steps {
               echo 'Test step'
           }
       }    
       stage('Deploy') {
           steps {
               echo 'Deploy step'
               sleep 10
           }
       }
       stage('Docker') {
           steps {
               echo 'Image step'
           }
       }
   }    
}   
       
    
    
    


